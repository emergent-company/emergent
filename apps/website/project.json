{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "website",
  "projectType": "application",
  "sourceRoot": "apps/website",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/website && task build"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/website && task dev"
      }
    },
    "serve-air": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/website && task dev-air"
      }
    },
    "css-rebuild": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/website && task css-rebuild"
      }
    },
    "install-deps": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/website && task install-deps"
      }
    },
    "fmt": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/website && task fmt"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/website && task lint"
      }
    }
  },
  "tags": ["type:app", "platform:go", "scope:website"]
}
